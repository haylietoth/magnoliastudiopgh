<header class="site-header">
  <a href="./">
    <div class="logo"><span style="font-family: horizon;">Magnolia </span><span style="font-family:vultures;font-weight:100;font-size: 4rem;vertical-align: middle;">studio</span></div>
  </a>
  <% if (menuContent != null) { %>
    <nav>
      <ul>
        <% menuContent.data.menu_links.map(menuLink => { %>
          <li>
            <a href="<%- ctx.prismicH.asLink(menuLink.link) %>">
              <%- ctx.prismicH.asText(menuLink.label) %>
            </a>
         </li>
        <% }) %>
      </ul>
    </nav>
  <% } %>
</header>
