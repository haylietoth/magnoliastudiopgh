<section class="site-header" id="site-header">
    <a href="./" class="logo">
      <div><span style="font-family: horizon;">Magnolia </span><span style="font-family:vultures;font-weight:100;font-size: 4rem;vertical-align: middle;">studio</span></div>
    </a>
    <div id="nav-bb">
      <% if (menuContent != null) { %>
        <% menuContent.data.menu_links.map(menuLink => { %>
            <a href="<%- ctx.prismicH.asLink(menuLink.link) %>" target="<%- menuLink.target %>">
              <%- ctx.prismicH.asText(menuLink.label) %>
            </a>
        <% }) %>
      <% } %>
    </div>
    <a href="javascript:void(0);" class="icon">
      <i class="fa fa-plus" aria-hidden="true"></i>
    </a>
</section>