<%- 
SliceZone.reduce((prevSlices, slice, index) => {
  if (slice.slice_type === 'text_section') {
     return prevSlices + include('./slices/TextSection.ejs', { slice: slice })
   }  
  else if (slice.slice_type === 'quote') {
    return prevSlices + include('./slices/Quote.ejs', { slice: slice })
  }
  else if (slice.slice_type === 'full_width_image') {
    return prevSlices + include('./slices/FullWidthImage.ejs', { slice: slice })
  }
  else if (slice.slice_type === 'image_gallery') {
    return prevSlices + include('./slices/ImageGallery.ejs', { slice: slice })
  }
  else if (slice.slice_type === 'image_highlight') {
    return prevSlices + include('./slices/ImageHighlight.ejs', { slice: slice })
  }
  else if (slice.slice_type === 'team_member') {
    return prevSlices + include('./slices/TeamMember.ejs', { slice: slice, index: index})
  }
  else if (slice.slice_type === 'service') {
    return prevSlices + include('./slices/Test.ejs', { slice: slice, index: index})
  }
  else if (slice.slice_type === 'slider') {
    return prevSlices + include('./slices/Slider.ejs', { slice: slice, index: index})
  }
  else {
    return '<h1>No Slices found</h1>';
  } 
}, '') %>

